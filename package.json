{
	"name": "movie-finder",
	"version": "1.0.0",
	"homepage": "https://danbamikiya.github.io/movie-finder",
	"description": "A web app for exploring movies, watching their trailers and checking out its actors!",
	"scripts": {
		"start": "cross-env NODE_ENV=development BABEL_ENV=development node scripts/start.js",
		"start:remote-dev": "cross-env REMOTE_DEV=true npm run start",
		"build": "cross-env NODE_ENV=production BABEL_ENV=production node scripts/build.js",
		"build:serve-local": "cross-env PUBLIC_URL='/' npm run build && node scripts/serve-local.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/danBamikiya/movie-finder.git"
	},
	"author": "Dan Bamikiya",
	"license": "MIT",
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"@babel/runtime": "^7.12.5",
		"cheerio": "^1.0.0-rc.9",
		"selector-observer": "^2.1.6"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-transform-async-to-generator": "^7.13.0",
		"@babel/preset-env": "^7.12.11",
		"@types/dotenv-webpack": "^7.0.2",
		"babel-loader": "^8.2.2",
		"case-sensitive-paths-webpack-plugin": "^2.4.0",
		"chalk": "^2.4.2",
		"core-js": "^3.8.2",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.6",
		"css-minimizer-webpack-plugin": "^3.0.1",
		"dotenv-webpack": "^7.0.1",
		"escape-string-regexp": "^1.0.5",
		"file-loader": "^6.2.0",
		"fork-ts-checker-webpack-plugin": "^6.2.10",
		"fs-extra": "^9.1.0",
		"html-webpack-plugin": "^5.3.1",
		"http-server": "^0.12.3",
		"inline-chunk-html-plugin": "^1.1.1",
		"mini-css-extract-plugin": "^1.6.0",
		"node-sass": "^6.0.1",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-loader": "^5.3.0",
		"postcss-normalize": "^10.0.0",
